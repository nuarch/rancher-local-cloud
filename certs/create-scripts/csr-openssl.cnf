default_bits        = 4096
distinguished_name  = req_distinguished_name
req_extensions      = req_ext

[ req_distinguished_name ]
C   = US
ST  = New York
L   = New York
O   = Local Rancher Cloud
OU  = Local Rancher Cloud Unit
CN  = local-rancher
